{
    "name": "dml",
    "displayName": "dml",
    "description": "Drash Markup Language Plugin",
    "version": "0.0.1",
    "engines": {
        "vscode": "^1.49.0"
    },
    "categories": [
        "Programming Languages"
    ],
    "contributes": {
        "languages": [{
            "id": "dml",
            "aliases": ["Drash Markup Language", "dml"],
            "extensions": [".dml"],
            "configuration": "./language-configuration.json"
        }],
        "grammars": [
            {
                "language": "dml",
                "scopeName": "source.dml",
                "path": "./syntaxes/dml.tmLanguage.json",
                "embeddedLanguages": {
                    "meta.embedded.block.html": "html",
                    "source.html": "html"
                }
            }
        ]
    }
}